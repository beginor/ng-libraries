{
  "name": "ng-seed",
  "version": "1.0.0",
  "description": "angular seed app",
  "main": "src/index.html",
  "scripts": {
    "clean": "rm -rf dist coverage",
    "ng": "ng",
    "build": "ng build ng-animations --prod && ng build esri-service --prod && ng build --prod",
    "lint": "ng lint",
    "start": "ng serve --host 0.0.0.0",
    "test": "ng test --watch",
    "test-coverage": "ng test --code-coverage",
    "e2e": "ng e2e --webdriver-update false",
    "webdriver-update": "webdriver-manager update --proxy http://127.0.0.1:10080"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beginor/ng-seed.git"
  },
  "keywords": [
    "angular",
    "seed",
    "angular-cli"
  ],
  "author": "beginor",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/beginor/ng-seed/issues"
  },
  "homepage": "https://github.com/beginor/ng-seed#readme",
  "dependencies": {
    "@angular/animations": "~7.0.0",
    "@angular/common": "~7.0.0",
    "@angular/compiler": "~7.0.0",
    "@angular/core": "~7.0.0",
    "@angular/forms": "~7.0.0",
    "@angular/platform-browser": "~7.0.0",
    "@angular/platform-browser-dynamic": "~7.0.0",
    "@angular/router": "~7.0.0",
    "core-js": "^2.5.4",
    "rxjs": "~6.3.3",
    "zone.js": "~0.8.26",
    "bootstrap": "^4.1.0",
    "@ng-bootstrap/ng-bootstrap": "^4.0.0",
    "@fortawesome/fontawesome-free": "^5.4.0",
    "esri-loader": "^2.5.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.10.0",
    "@angular/cli": "~7.0.1",
    "@angular/compiler-cli": "~7.0.0",
    "@angular/language-service": "~7.0.0",
    "@types/node": "~8.9.4",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.1",
    "@angular-devkit/build-ng-packagr": "~0.7.0",
    "ng-packagr": "^3.0.0",
    "tsickle": ">=0.29.0",
    "tslib": "^1.9.0",
    "@types/arcgis-js-api": "^4.9.0"
  }
}
